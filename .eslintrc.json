{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["react", "import", "jsx-a11y"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/prop-types": 0,
    "indent": ["error", 2],
    "linebreak-style": 0,
    "quotes": ["error", "double"],
    "react/react-in-jsx-scope": 0,
    "import/no-unresolved": 0,
    "no-unused-vars": 2,
    "react-hooks/exhaustive-deps": 0,
    "no-unknown-property": 0,
    "camelcase": 2,
    "indent-legacy": 0
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
